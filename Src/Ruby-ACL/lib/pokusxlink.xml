<acl xmlns:xlink="http://www.w3.org/1999/xlink" aclname="my_first_acl">
    <Principals>
        <Groups>
            <group id="all"/>
            <group id="Developers">
                <membership>    <!-->Developers are members of Administrators<!-->
                    <group xlink:type="simple" xlink:href="acl.xml#Administrators"/>
                    <group xlink:type="simple" xlink:href="acl.xml#Users"/>
                </membership>
            </group>
            <group id="Administrators">
                <membership/>
            </group>
            <group id="Users">
                <membership/>
            </group>
            <group id="labutiHejno">
                <membership>
                    <group xlink:type="simple" xlink:href="acl.xml#Users"/>
                </membership>
            </group>
        </Groups>
        <Individuals>
            <principal id="sirljan">
                <membership>
                    <group xlink:type="simple" xlink:href="acl.xml#Users"/>
                    <group xlink:type="simple" xlink:href="acl.xml#Developers"/>
                </membership>
            </principal>
            <principal id="labut">
                <membership>
                    <group xlink:type="simple" xlink:href="acl.xml#labutiHejno"/>
                    <group xlink:type="simple" xlink:href="acl.xml#Administrators"/>
                    <group xlink:type="simple" xlink:href="acl.xml#Users"/>
                    <group xlink:type="simple" xlink:href="acl.xml#Developers"/>
                    <group xlink:type="simple" xlink:href="acl.xml#Houbari"/>
                </membership>
            </principal>
        </Individuals>
    </Principals>
    <Privileges>
        <allow>
            <privilege>
                <name>ALL PRIVILEGES</name>
            </privilege>
            <privilege>
                <name>ALTER</name>
            </privilege>
            <privilege>
