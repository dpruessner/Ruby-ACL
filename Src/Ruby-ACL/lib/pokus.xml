<acl xmlns:xlink="http://www.w3.org/1999/xlink" aclname="my_first_acl">
    <Principals>
        <Groups>
            <group id="all"/>
            <group id="Developers">
                <membership>    <!-->Developers are members of Administrators<!-->
                    <group idref="Users"/>
                </membership>
            </group>
            <group id="Administrators">
                <membership/>
            </group>
            <group id="Users">
                <membership>
                    <group idref="all"/>
                </membership>
            </group>
            <group id="labutiHejno">
                <membership>
                    <group idref="Users"/>
                </membership>
            </group>
        </Groups>
        <Individuals>
            <principal id="sirljan">
                <membership>
                    <group idref="Users"/>
                    <group idref="Developers"/>
                </membership>
            </principal>
            <principal id="labut">
                <membership>
                    <group idref="labutiHejno"/>
                    <group idref="Administrators"/>
                    <group idref="Developers"/>
                    <group idref="Houbari"/>
                </membership>
            </principal>
        </Individuals>
    </Principals>
    <Privileges>
        <privilege id="ALL PRIVILEGES"/>
        <privilege id="ALTER">
            <membership/>
        </privilege>
        <privilege id="CREATE">
            <membership/>
        </privilege>
        <privilege id="DELETE">
            <membership/>
        </privilege>
        <privilege id="DROP">
            <membership/>
        </privilege>
        <privilege id="FILE">
            <membership/>
        </privilege>
        <privilege id="INDEX">
            <membership/>
        </privilege>
        <privilege id="INSERT">
            <membership/>
        </privilege>
        <privilege id="PROCESS">
            <membership/>
        </privilege>
        <privilege id="REFERENCES">
            <membership/>
        </privilege>
        <privilege id="RELOAD">
            <membership/>
        </privilege>
        <privilege id="SELECT">
            <membership/>
        </privilege>
        <privilege id="SHUTDOWN">
            <membership/>
        </privilege>
        <privilege id="UPDATE">
            <membership/>
        </privilege>
        <privilege id="USAGE">
            <membership/>
        </privilege>
        <privilege id="KUTALET">
            <membership/>
        </privilege>
        <privilege id="STAT">
            <membership>
                <privilege idref="KUTALET"/>
            </membership>
        </privilege>
    </Privileges>
    <ResourceObjects>
        <resourceObject id="456">
            <name>Kosik</name>
            <address>/Kosik</address>
        </resourceObject>
        <resourceObject id="963">
            <name>Les</name>
            <address>/Les</address>
        </resourceObject>
        <resourceObject id="852">
            <name>cities.xml</name>
            <address>/db/cities</address>
        </resourceObject>
    </ResourceObjects>
    <ACEs>
        <ace id="987">
            <principal idref="Klubicko"/>
            <accessType>allow</accessType>
            <privilege idref="KUTALET"/>
            <resourceObject idref="456"/>
        </ace>
        <ace id="894">
            <principal idref="Users"/>
            <accessType>allow</accessType>
            <privilege idref="SELECT"/>
            <resourceObject idref="852"/>
        </ace>
        <ace id="0">
            <principal idref="Houby"/>
            <accessType>allow</accessType>
            <privilege idref="RUST"/>
            <resourceObject idref="963"/>
        </ace>
    </ACEs>
</acl>
