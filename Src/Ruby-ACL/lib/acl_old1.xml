
<acl aclname="my_first_acl">
    <Principals>
        <Groups>
            <principal>
                <name>all</name>
            </principal>
            <principal>
                <name>Developers</name>
                <members>
                    <member>sirljan</member>
                </members>
            </principal>
            <principal>
                <name>Administrators</name>
            </principal>
            <principal>
                <name>Users</name>
                <members>
                    <member>sirljan</member>
                </members>
            </principal>
        </Groups>
        <Individuals>
            <principal>
                <name>sirljan</name>
                <member_of>
                    <group>Developers</group>
                    <group>Users</group>
                </member_of>
            </principal>
        </Individuals>

    </Principals>
    
    <Privileges>
        <allow>
            <privilege>
                <name>ALL PRIVILEGES</name>
            </privilege>
            <privilege>
                <name>ALTER</name>
            </privilege>
            <privilege>
                <name>CREATE</name>
            </privilege>
            <privilege>
                <name>DELETE</name>
            </privilege>
            <privilege>
                <name>DROP</name>
            </privilege>
            <privilege>
                <name>FILE</name>
            </privilege>
            <privilege>
                <name>INDEX</name>
            </privilege>
            <privilege>
                <name>INSERT</name>
            </privilege>
            <privilege>
                <name>PROCESS</name>
            </privilege>
            <privilege>
                <name>REFERENCES</name>
            </privilege>
            <privilege>
                <name>RELOAD</name>
            </privilege>
            <privilege>
                <name>SELECT</name>
            </privilege>
            <privilege>
                <name>SHUTDOWN</name>
            </privilege>
            <privilege>
                <name>UPDATE</name>
            </privilege>
            <privilege>
                <name>USAGE</name>
            </privilege>            
        </allow>
        <deny>
            <privilege>
                <name>ALL PRIVILEGES</name>
            </privilege>
            <privilege>
                <name>ALTER</name>
            </privilege>
            <privilege>
                <name>CREATE</name>
            </privilege>
            <privilege>
                <name>DELETE</name>
            </privilege>
            <privilege>
                <name>DROP</name>
            </privilege>
            <privilege>
                <name>FILE</name>
            </privilege>
            <privilege>
                <name>INDEX</name>
            </privilege>
            <privilege>
                <name>INSERT</name>
            </privilege>
            <privilege>
                <name>PROCESS</name>
            </privilege>
            <privilege>
                <name>REFERENCES</name>
            </privilege>
            <privilege>
                <name>RELOAD</name>
            </privilege>
            <privilege>
                <name>SELECT</name>
            </privilege>
            <privilege>
                <name>SHUTDOWN</name>
            </privilege>
            <privilege>
                <name>UPDATE</name>
            </privilege>
            <privilege>
                <name>USAGE</name>
            </privilege>
        </deny>
    </Privileges>
    
    <ResourceObjects>    
    </ResourceObjects>
    
    <aces>
    </aces>
</acl>