            </privilege>
            <privilege>
                <name>INSERT</name>
            </privilege>
            <privilege>
                <name>PROCESS</name>
            </privilege>
            <privilege>
                <name>REFERENCES</name>
            </privilege>
            <privilege>
                <name>RELOAD</name>
            </privilege>
            <privilege>
                <name>SELECT</name>
            </privilege>
            <privilege>
                <name>SHUTDOWN</name>
            </privilege>
            <privilege>
                <name>UPDATE</name>
            </privilege>
            <privilege>
                <name>USAGE</name>
            </privilege>
